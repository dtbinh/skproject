for n=1:length(CFcombiselect242)
    x=CFcombiselect242(n).ISRx;
    y=CFcombiselect242(n).ISRy;
    level=CFcombiselect242(n).SPL1(1)
    if level==70
        plot(x,y,'r');hold on;
    elseif level==60
        plot(x,y,'m');hold on;
    elseif level==50
        plot(x,y,'b');hold on;
    elseif level==40
        plot(x,y,'c');hold on;
    elseif level==30
        plot(x,y,'k');hold on;
    end;
end;
figure
